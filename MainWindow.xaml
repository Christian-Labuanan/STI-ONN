<Window x:Name="window" x:Class="STI_ONN.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:STI_ONN"
        mc:Ignorable="d"
        Title="STI ONN" Height="auto" Width="auto">
    <Grid Background="#FFEAF2FF" HorizontalAlignment="Center" VerticalAlignment="Center" Height="{Binding ElementName=window, Mode=OneWay}">
        <Rectangle HorizontalAlignment="Center" Height="49" Margin="0,385,0,0" Stroke="#0B5793" Fill="#0B5793" VerticalAlignment="Top" Width="800"/>
        <Image x:Name="STI_ONN" Height="350" Width="500" Source="/STI ONN Logo transparent 3.png" Stretch="Fill" Margin="150,42,150,42"/>
        <Button x:Name="touch" Content="Touch to Start" HorizontalAlignment="Center" Margin="0,348,0,0" VerticalAlignment="Top" Height="62" Width="322"/>
    </Grid>
</Window>
